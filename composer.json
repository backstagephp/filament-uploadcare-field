{
    "name": "vormkracht10/filament-uploadcare-field",
    "description": "Uploadcare FileUpload component for Filament Forms",
    "keywords": [
        "vormkracht10",
        "laravel",
        "filament-uploadcare-field"
    ],
    "homepage": "https://github.com/vormkracht10/filament-uploadcare-field",
    "support": {
        "issues": "https://github.com/vormkracht10/filament-uploadcare-field/issues",
        "source": "https://github.com/vormkracht10/filament-uploadcare-field"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Baspa",
            "email": "hello@baspa.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Vormkracht10\\Uploadcare\\": "src/",
            "Vormkracht10\\Uploadcare\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vormkracht10\\Uploadcare\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vormkracht10\\Uploadcare\\UploadcareServiceProvider"
            ],
            "aliases": {
                "Uploadcare": "Vormkracht10\\Uploadcare\\Facades\\Uploadcare"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
